{
  "name": "neessi",
  "version": "11.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "echo:env": "node ./scripts/echoenv.js",
    "patch:init": "node ./scripts/patchinit.js",
    "patch:env": "node ./scripts/patchenv.js",
    "patch:nodelocal": "node ./scripts/patchnodelocal.js",
    "patch:javalocal": "node ./scripts/patchjavalocal.js",
    "npx": "npx npm-force-resolutions",
    "build": "npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p build:js",
    "build:javalocal": "npm-run-all -s patch:init patch:env patch:javalocal echo:env -p build:js",
    "build:js": "NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "lint": "standardx './src/**/*.ts' './src/**/*.tsx'",
    "lint:fix": "standardx './src/**/*.ts' './src/**/*.tsx' --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p start:js",
    "start:javalocal": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:javalocal echo:env -p start:js",
    "start:js": "cross-env PORT=3003 NODE_OPTIONS=--max-old-space-size=4096 vite",
    "test": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test jest",
    "test:clear": "cross-env NODE_ENV=test jest --clearCache",
    "test:ci": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test jest --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage  --watchAll=false",
    "preview": "vite preview",
    "knip": "knip"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.35.1",
    "@navikt/ds-css": "7.35.1",
    "@navikt/ds-react": "7.35.1",
    "@navikt/eessi-kodeverk": "2.4.1",
    "@navikt/fetch": "4.0.15",
    "@navikt/flagg-ikoner": "10.0.19",
    "@navikt/fnrvalidator": "2.2.1",
    "@navikt/hoykontrast": "4.0.1",
    "@navikt/land-verktoy": "9.1.28",
    "@navikt/landvelger": "10.0.11",
    "@navikt/tabell": "9.0.3",
    "@reduxjs/toolkit": "1.9.7",
    "ansi-to-html": "0.7.2",
    "bytes": "3.1.2",
    "classnames": "2.5.1",
    "connect-timeout": "1.9.1",
    "core-js": "3.47.0",
    "cross-fetch": "3.1.5",
    "dayjs": "1.11.19",
    "eux-schema": "0.4.0",
    "express": "5.2.1",
    "file-saver": "2.0.5",
    "http-proxy-middleware": "2.0.9",
    "i18next": "21.10.0",
    "i18next-browser-languagedetector": "6.1.8",
    "i18next-xhr-backend": "3.2.2",
    "jose": "4.15.9",
    "lodash": "4.17.21",
    "md5": "2.3.0",
    "moment": "2.29.4",
    "openid-client": "5.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-i18next": "11.18.6",
    "react-pdf": "9.2.1",
    "react-redux": "8.0.5",
    "react-router-dom": "7.9.6",
    "react-select": "5.7.0",
    "redux": "4.2.0",
    "sprintf-js": "1.1.2",
    "stacktracey": "2.1.8",
    "styled-components": "5.3.11",
    "url": "0.11.4",
    "winston": "3.19.0",
    "worker-timers": "8.0.24"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/bytes": "3.1.5",
    "@types/file-saver": "2.0.7",
    "@types/jest": "28.1.8",
    "@types/lodash": "4.17.21",
    "@types/md5": "2.3.6",
    "@types/node": "^24.10.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-redux": "7.1.25",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.34.0",
    "@vitejs/plugin-react": "5.1.1",
    "cross-env": "7.0.3",
    "dotenv-save": "1.2.0",
    "eslint": "8.31.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.1.1",
    "jest-transformer-svg": "2.1.0",
    "knip": "^5.70.2",
    "npm-run-all": "4.1.5",
    "prop-types": "15.8.1",
    "read-pkg": "7.1.0",
    "shelljs": "0.8.5",
    "simple-git": "3.30.0",
    "standardx": "7.0.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "write-pkg": "5.1.0"
  },
  "resolutions": {},
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "fragment": "Fragment",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "standardx": {
    "ignore": [
      "public/",
      "*.snap"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "globals": [
      "act",
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "Blob",
      "CustomEvent",
      "Event",
      "describe",
      "expect",
      "FileReader",
      "fetch",
      "Element",
      "HTMLCanvasElement",
      "HTMLButtonElement",
      "HTMLInputElement",
      "HTMLSelectElement",
      "HTMLTextAreaElement",
      "it",
      "jest",
      "JSX",
      "Location",
      "mount",
      "ReactDOM",
      "render",
      "renderer",
      "shallow"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "readme": "ERROR: No README data found!",
  "_id": "neessi@11.0.0",
  "disabled-homepage": "http://eesi2.no/melosys"
}
