{
  "name": "neessi",
  "version": "1.1.1",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "eessi-kodeverk": "1.4.2",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#2.2.8",
    "history": "^4.10.1",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "proxy": "http://localhost:3002",
  "scripts": {
    "npx": "npx npm-force-resolutions",
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "lint": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts* --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env PORT=3003 NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "devDependencies": {
    "@navikt/fnrvalidator": "^1.1.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^7.0.2",
    "dotenv-save": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.11.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-axe": "^3.4.1",
    "redux-thunk": "^2.3.0",
    "standardx": "^5.0.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "minimist": "1.2.3"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "globals": [
      "act",
      "Blob",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/pages/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/utils/*.{ts,tsx}"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
