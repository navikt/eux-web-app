{
  "name": "eux-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "patchenv": "node ./scripts/patchenv.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "autoless src",
    "build:css": "autoless --no-watch src",
    "start": "npm run patchenv && npm-run-all -p start:*",
    "build": "npm run patchenv && npm run build:css && npm run build:js",
    "test": "npm run build:css && react-scripts test --env=jsdom",
    "test:ci": "npm run build:css && react-scripts test --env=jsdom --testResultsProcessor ./node_modules/jest-junit --coverage --coverageDirectory=test/coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint src --ext .js",
    "lesshint": "lesshint --reporter ./lesshint-reporter src/",
    "mocha": "mocha"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "moment": "^2.21.0",
    "moment-duration-format": "^2.2.2"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.3",
    "dotenv-save": "^1.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "git-branch": "^2.0.1",
    "jest-cli": "^21.2.1",
    "jest-junit": "^3.3.0",
    "lesshint": "^4.1.3",
    "lint-staged": "^4.2.3",
    "mockdate": "^2.0.2",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^16.0.0",
    "semver": "^5.4.1",
    "source-map-explorer": "^1.5.0",
    "underscore": "^1.8.3",
    "webpack": "^3.10.0"
  }
}
