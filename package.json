{
  "name": "neessi",
  "version": "1.1.1",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "eessi-kodeverk": "1.4.2",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#2.1.9",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.0",
    "react-redux-form-validation": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-freeze": "^0.1.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "less-watch-compiler --config less.json",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "start": "npm-run-all -p start:*",
    "build": "npm run build:css && npm run build:js",
    "test": "npm run build:css && react-scripts-ts test --env=jsdom",
    "test:ci": "npm run build:css && react-scripts test --env=jsdom --testResultsProcessor ./node_modules/jest-junit --coverage --coverageDirectory=test/coverage",
    "eslint": "eslint src --ext .js"
  },
  "devDependencies": {
    "@navikt/fnrvalidator": "^1.1.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "^7.0.0",
    "dotenv-save": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "less": "^3.11.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "standardx": "^5.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "proxy": "http://localhost:3002"
}
