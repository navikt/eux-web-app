{
  "name": "neessi",
  "version": "7.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "echo:env": "node ./scripts/echoenv.js",
    "patch:init": "node ./scripts/patchinit.js",
    "patch:env": "node ./scripts/patchenv.js",
    "patch:nodelocal": "node ./scripts/patchnodelocal.js",
    "patch:javalocal": "node ./scripts/patchjavalocal.js",
    "serve": "node ./scripts/serve.js",
    "serve:dbg": "node $NODE_DEBUG_OPTION ./scripts/serve.js",
    "npx": "npx npm-force-resolutions",
    "build": "npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p build:js",
    "build:javalocal": "npm-run-all -s patch:init patch:env patch:javalocal echo:env -p build:js",
    "build:js": "NODE_OPTIONS=--max-old-space-size=4096 react-scripts build",
    "lint": "standardx './src/**/*.ts' './src/**/*.tsx'",
    "lint:fix": "standardx './src/**/*.ts' './src/**/*.tsx' --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p start:js",
    "start:javalocal": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:javalocal echo:env -p start:js",
    "start:js": "cross-env PORT=3003 NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "dependencies": {
    "@navikt/ds-css": "^0.18.26",
    "@navikt/ds-icons": "^0.8.18",
    "@navikt/ds-react": "^0.19.19",
    "@navikt/eessi-kodeverk": "^2.0.0",
    "@navikt/fetch": "^2.1.8",
    "@navikt/flagg-ikoner": "^3.2.0",
    "@navikt/fnrvalidator": "^1.3.0",
    "@navikt/forhandsvisningsfil": "^3.0.4",
    "@navikt/fremdriftslinje": "^3.0.2",
    "@navikt/hoykontrast": "^3.1.0",
    "@navikt/land-verktoy": "^3.1.2",
    "@navikt/landvelger": "^3.1.0",
    "@navikt/paginering": "^3.0.1",
    "@navikt/tabell": "^3.5.4",
    "@navikt/tooltip": "^1.1.1",
    "@reduxjs/toolkit": "^1.8.3",
    "@sentry/browser": "^7.5.0",
    "amplitude-js": "^8.18.5",
    "ansi-to-html": "^0.7.2",
    "classnames": "^2.3.1",
    "core-js": "^3.23.3",
    "eux-schema": "^0.4.0",
    "file-saver": "^2.0.5",
    "i18next": "^21.8.12",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "nav-frontend-tabell-style": "^2.1.1",
    "rc-tooltip": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.17.4",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "stacktracey": "^2.1.8",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/amplitude-js": "^8.16.1",
    "@types/classnames": "^2.3.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "cross-env": "^7.0.3",
    "dotenv-save": "^1.2.0",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "jest": "^28.1.2",
    "jest-styled-components": "^7.0.8",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react-axe": "^3.5.3",
    "react-scripts": "^5.0.1",
    "read-pkg": "^7.1.0",
    "shelljs": "^0.8.5",
    "simple-git": "^3.10.0",
    "standardx": "^7.0.0",
    "typescript": "^4.7.4",
    "write-pkg": "^5.1.0"
  },
  "resolutions": {},
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "fragment": "Fragment",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "standardx": {
    "ignore": [
      "public/",
      "*.snap"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "globals": [
      "act",
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "Blob",
      "CustomEvent",
      "Event",
      "describe",
      "expect",
      "FileReader",
      "fetch",
      "Element",
      "HTMLCanvasElement",
      "HTMLButtonElement",
      "HTMLInputElement",
      "HTMLSelectElement",
      "HTMLTextAreaElement",
      "it",
      "jest",
      "JSX",
      "Location",
      "mount",
      "ReactDOM",
      "render",
      "renderer",
      "shallow"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/pages/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/utils/*.{ts,tsx}"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "readme": "ERROR: No README data found!",
  "_id": "neessi@7.0.0-beta.0",
  "disabled-homepage": "http://eesi2.no/melosys"
}
