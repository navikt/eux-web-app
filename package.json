{
  "name": "neessi",
  "version": "2.0.2",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "eessi-kodeverk": "1.4.2",
    "eessi-pensjon-ui": "git+https://github.com/navikt/eessi-pensjon-ui.git#2.4.1",
    "history": "^4.10.1",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.26.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "use-error-boundary": "^1.2.3"
  },
  "proxy": "http://localhost:3002",
  "scripts": {
    "echo:env": "node ./scripts/echoenv.js",
    "patch:init": "node ./scripts/patchinit.js",
    "patch:env": "node ./scripts/patchenv.js",
    "patch:nodelocal": "node ./scripts/patchnodelocal.js",
    "patch:javalocal": "node ./scripts/patchjavalocal.js",
    "serve": "node ./scripts/serve.js",
    "serve:dbg": "node $NODE_DEBUG_OPTION ./scripts/serve.js",
    "npx": "npx npm-force-resolutions",
    "build": "npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p build:css build:js",
    "build:javalocal": "npm-run-all -s patch:init patch:env patch:javalocal echo:env -p build:css build:js",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "NODE_OPTIONS=--max-old-space-size=4096 react-scripts build",
    "lint": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts* --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:nodelocal echo:env -p start:css start:js",
    "start:javalocal": "cross-env NODE_ENV=development npm-run-all -s patch:init patch:env patch:javalocal echo:env -p start:css start:js",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env PORT=3003 NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "npm run patch:init && npm run patch:env && cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "devDependencies": {
    "@navikt/fnrvalidator": "^1.1.2",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.153",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "dotenv-save": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "git-branch": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.11.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mockdate": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-axe": "^3.4.1",
    "read-pkg": "^5.2.0",
    "standardx": "^5.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "write-pkg": "^4.0.0"
  },
  "resolutions": {
    "yargs-parser": "18.1.2"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "globals": [
      "act",
      "Blob",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "src/actions": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/actions/*.{ts,tsx}",
      "src/applications/**/*.{ts,tsx}",
      "src/components/**/*.{ts,tsx}",
      "src/pages/**/*.{ts,tsx}",
      "src/reducers/*.{ts,tsx}",
      "src/utils/*.{ts,tsx}"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "readme": "ERROR: No README data found!",
  "_id": "neessi@2.0.2",
  "disabled-homepage": "http://eesi2.no/melosys"
}
