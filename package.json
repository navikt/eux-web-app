{
  "name": "neessi",
  "version": "1.1.1",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "eessi-kodeverk": "1.4.2",
    "eessi-pensjon-ui": "https://github.com/navikt/eessi-pensjon-ui#2.1.9",
    "history": "^4.10.1",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.3",
    "react-modal": "^3.11.1",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.0",
    "react-redux-form-validation": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-freeze": "^0.1.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js && npm run build:git",
    "build:git": "export GIT_VERSION=`git describe --always` && sed -i -- \"s/%GIT_VERSION%/$GIT_VERSION/g\" build/index.html",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "build:js": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "lint": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* src/*/*/*.ts* src/*/*/*/*.ts* src/*/*/*/*/*.ts* --fix",
    "start": "cross-env NODE_ENV=development npm-run-all -p start:*",
    "start:css": "less-watch-compiler --config less.json",
    "start:js": "cross-env PORT=3003 NODE_OPTIONS=--max-old-space-size=4096 react-scripts start",
    "test": "cross-env NODE_ENV=test react-scripts test",
    "test:clear": "cross-env NODE_ENV=test react-scripts test --clearCache",
    "test:ci": "cross-env NODE_ENV=test react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "cross-env NODE_ENV=test react-scripts test --coverage  --watchAll=false"
  },
  "devDependencies": {
    "@navikt/fnrvalidator": "^1.1.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-form": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "^7.0.0",
    "dotenv-save": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.11.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-axe": "^3.4.1",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "standardx": "^5.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "ignore": [
      "public/**/*.js",
      "node_modules/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "proxy": "http://localhost:3002"
}
